--SELECT ALL RECORDS FROM TABLE REPORTS THAT HAVE A CREATE_DATE BEFORE JULY 9TH 2006 IN NEWEST - OLDEST ORDER

--NEW to OLD
$sqlQuery = "SELECT * FROM REPORTS WHERE CREATE_DATE < 2006-09-07 ORDER BY DESC";

--OLD to NEW
$sqlQuery = "SELECT * FROM REPORTS WHERE CREATE_DATE < 2006-09-07 ORDER BY ASC";

