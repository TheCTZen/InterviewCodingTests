  public class Factorial {
  //  CALCULATE FACTORIAL â€“ JAVA (Using recursive method)
  
    public static void main(String[] args) {
    System.out.println("Factorial of number 5 is:"+ " " + calculateFactorial(5));
    }

    public static long calculateFactorial(long n){
      if (n <= 1)
      return 1;
      else
      return n * calc(n-1);
    }
  }
